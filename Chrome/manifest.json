{
    "manifest_version": 2,
    
    "name": "FikaNote Extension",
    "description": "This extension to post current url to FikaNote agenda.",
    "version": "1.0",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "permissions": [
	"tabs",
	"http://*/*", 
	"https://*/*"
    ]
}
